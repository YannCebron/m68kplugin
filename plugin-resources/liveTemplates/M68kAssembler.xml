<templateSet group="M68k Assembler">
  <template name="ifnd" value=" IFND $NAME$&#10;$NAME$ SET 1&#10; ENDC" description="IFND" toReformat="true"
            toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="M68K" value="true"/>
    </context>
  </template>
  <template name="str" value="dc.b &quot;$CARET$&quot;,0" description="String constant (0-terminated)"
            toReformat="true" toShortenFQNames="true">
    <variable name="CARET" expression="" defaultValue="" alwaysStopAt="true"/>
    <context>
      <option name="M68K" value="true"/>
    </context>
  </template>
</templateSet>